{
  "env": {
      "browser": true,
      "react-native/react-native": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-native-a11y/ios",
      "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "overrides": [],
  "parserOptions": {
      "ecmaVersion": "latest",
      "ecmaFeatures": {
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "react-native",
      "prettier",
  ],
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "rules": {
      "prettier/prettier": [
          "error"
      ]
  }
}